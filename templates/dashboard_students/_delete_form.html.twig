<form method="post" action="{{ path('app_dashboard_students_delete', {'id': student.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet étudiant ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ student.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
