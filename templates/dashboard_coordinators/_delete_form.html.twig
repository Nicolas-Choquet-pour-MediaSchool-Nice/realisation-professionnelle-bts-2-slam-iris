<form method="post" action="{{ path('app_dashboard_coordinators_delete', {'id': coordinator.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce coordinateur ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ coordinator.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
